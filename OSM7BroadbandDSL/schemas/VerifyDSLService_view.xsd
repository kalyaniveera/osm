                                <xs:element name="serviceActioncode" type="xs:string"/>
                                <xs:element name="name" type="xs:string"/>
                                <xs:element name="dataTypeCode" type="xs:string"/>
                                <xs:element name="value" type="xs:string"/>
                              </xs:sequence>
                            </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
