<process xmlns="http://www.metasolv.com/OMS/OrderModel/2002/06/25" name="AddDSLProcess" workstream="false">
  <description>AddDSLProcess</description>
  <expectedDuration>P0Y0M0DT0H0M86400S</expectedDuration>
  <startActivity>VerifyDSLService</startActivity>
  <activity name="ActivateDSLAM">
    <task>ActivateDSLAM</task>
    <description>ActivateDSLAM</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="AddExtraCapacity">
    <task>AddExtraCapacity</task>
    <description>AddExtraCapacity</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="AssignDSLPort">
    <task>AssignDSLPort</task>
    <description>AssignDSLPort</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="ShipDSLModem">
    <task>ShipDSLModem</task>
    <description>ShipDSLModem</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="VerifyDSLService">
    <task>VerifyDSLService</task>
    <description>VerifyDSLService</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="VerifyOrder">
    <task>VerifyOrder</task>
    <description>VerifyOrder</description>
    <inputType>ANY</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <activity name="WaitForPortAndModem">
    <task>join_task</task>
    <description>WaitForPortAndModem</description>
    <inputType>ALL</inputType>
    <processHistorySummary>true</processHistorySummary>
  </activity>
  <transition>
    <from>
      <activity>VerifyDSLService</activity>
      <status>success</status>
    </from>
    <to>ShipDSLModem</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>VerifyDSLService</activity>
      <status>success</status>
    </from>
    <to>AssignDSLPort</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>AssignDSLPort</activity>
      <status>port_unavailable</status>
    </from>
    <to>AddExtraCapacity</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>AddExtraCapacity</activity>
      <status>next</status>
    </from>
    <to>AssignDSLPort</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>AssignDSLPort</activity>
      <status>port_available</status>
    </from>
    <to>WaitForPortAndModem</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>ShipDSLModem</activity>
      <status>completed</status>
    </from>
    <to>WaitForPortAndModem</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>WaitForPortAndModem</activity>
      <status>true</status>
    </from>
    <to>ActivateDSLAM</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>ActivateDSLAM</activity>
      <status>success</status>
    </from>
    <to>VerifyOrder</to>
    <mandatoryCheck>true</mandatoryCheck>
  </transition>
</process>